      __bang_transpose(nram_input, nram_output, 2, 2*num_deal);

      __bang_printf("output_nram num: %d \n", 2*actual_block_num * actual_num_deal);
      for (int j = 0; j < 2*actual_block_num * actual_num_deal;j++) {
