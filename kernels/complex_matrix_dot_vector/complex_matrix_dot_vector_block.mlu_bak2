      __bang_add(nram_temp, nram_temp, nram_temp + block_imag_offset, actual_block_num * actual_num_deal);
      // __bang_printf("\nimag part\n");
      // for (int j = 0; j < actual_block_num * actual_num_deal;j++) {
      //   __bang_printf("%f ", nram_temp[j]);
      // }
